<span tal:omit-tag="">
    <input type="text"
           name="${name}"
           value="${cstruct}" 
           tal:attributes="size size;
                           class css_class"
           id="${oid}"/>
    <script tal:condition="values" type="text/javascript">
      deform.addCallback(
        '${oid}',
        function (oid) {
            $('#' + oid).autocomplete({source: ${values}});
            $('#' + oid).autocomplete("option", ${options});
        }
      );
    </script>
</span>
