<div tal:omit-tag="">
${field.start_sequence()}
<ul class="deformSet">
  <tal:loop tal:repeat="choice values">
    <tal:def tal:define="(value, title) choice">
      <li class="deformSet-item">
        <input tal:attributes="checked value in cstruct;
                               class css_class"
               type="checkbox"
               name="checkbox"
               value="${value}"
               id="${oid}-${repeat.choice.index}"/>
        <label for="${oid}-${repeat.choice.index}">${title}</label>
      </li>
    </tal:def>
  </tal:loop>
</ul>
${field.end_sequence()}

