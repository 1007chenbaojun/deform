<div tal:define="
     oid oid|field.oid;
     true_val true_val|field.widget.true_val;"
     i18n:domain="deform">
  <div class="deform-readonly-true" 
       id="${oid}"
       tal:condition="cstruct == true_val"
       i18n:translate="">True</div>
  <div class="deform-readonly-false" 
       id="${oid}"
       tal:condition="cstruct != true_val"
       i18n:translate="">False</div>
</div>
